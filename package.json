    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.2",
    "jest-mock-extended": "2.0.4",
    "ngrok": "^4.3.1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "prisma": "^4.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
