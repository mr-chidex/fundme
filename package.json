{
  "name": "node_test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn build && node ./dist/index.js",
    "dev": "yarn clean && nodemon ./src/index.ts",
    "build": "tsc -p . && prisma generate",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "dotenv -e .env.test jest -i --detectOpenHandles --forceExit --verbose",
    "test:watch": "jest --watchAll  --forceExit --detectOpenHandles",
    "lint": "eslint src/**/*.ts",
    "lint:format": "eslint src/**/*.ts --fix",
    "serve": "yarn ngrok http 8080",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name testdb"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
